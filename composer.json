{
    "name": "php-core/abaninja-sdk",
    "description": "AbaNinja.ch PHP SDK",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PHPCore\\AbaNinja\\": "src/PHPCore/AbaNinja"
        },
        "files": [
            "src/PHPCore/AbaNinja/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "PHPCore\\AbaNinja\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "php-core",
            "email": "dev@php-core.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3.1",
        "vlucas/phpdotenv": "v5.6.1"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --display-warnings --display-notices --display-skipped"
    }
}
